package coordinates;

public class Coordinates {
    int row;
    int col;
    public Coordinates(int row,int col){
        this.row=row;
        this.col=col;
    }
    public String printCoordinates(){
        StringBuilder st=new StringBuilder();
        st.append("[").append(this.row).append(",").append(this.col).append("]");
        return st.toString();
    }
    public int getRow(){
        return this.row;
    }
    public int getCol(){
        return this.col;
    }
}
